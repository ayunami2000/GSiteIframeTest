<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Preferences for iframe" height="250" />
  <UserPref name="url" display_name="URL" required="true" />
  <UserPref name="width" display_name="iframe width" default_value="100%"/>
  <UserPref name="height" display_name="iframe height" default_value="50%"/>
  <Content type="html">
  <![CDATA[
    <script>
      gadgets.util.registerOnLoadHandler(function(){try{
      for(i=0;i<window.parent.document.querySelectorAll('iframe').length;i++){
      if(window.parent.document.querySelectorAll('iframe')[i].contentDocument==window.document){
      window.parent.document.querySelectorAll('iframe')[i].width=window.parent.document.querySelectorAll('iframe')[i].style.width="__UP_width__";
      window.parent.document.querySelectorAll('iframe')[i].height=window.parent.document.querySelectorAll('iframe')[i].style.height="__UP_height__";
      }
      }
      window.location.href="__UP_url__";
      }catch(e){top.console.log(e);}});
    </script>
  ]]>
  </Content>
</Module>
