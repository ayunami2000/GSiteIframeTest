<?xml version="1.0" encoding="UTF-8" ?>
<Module>
  <ModulePrefs title="Preferences for iframe" height="250" />
  <UserPref name="url" display_name="URL" required="true" />
  <UserPref name="width" display_name="iframe width" default_value="100%"/>
  <UserPref name="height" display_name="iframe height" default_value="50%"/>
  <Content type="html">
  <![CDATA[
    <script>
      document.parentWindow.self.width=__UP_width__;
      document.parentWindow.self.style.width=__UP_width__;
      document.parentWindow.self.height=__UP_height__;
      document.parentWindow.self.style.height=__UP_height__;
      this.location=__UP_url__;
    </script>
  ]]>
  </Content>
</Module>
